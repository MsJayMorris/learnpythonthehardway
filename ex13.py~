#imports the arv module from the sys library
from sys import argv

#assigns the parameters passed in at the command line to each of the variables in order
script, first, second = argv

#Prints the actual .py file name
print "This script is called:", script

#Prints the arguments passed in at the command line to the associated variable it was assigned to 
print "Your first variable is:", first
print "Your second variable is:", second


y = raw_input("Hey baby whats your name? ")
print y
x = raw_input("How old are you?")
print x
