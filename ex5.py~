name = 'Janice D. Morris'
age = 36
weight = 200
height = 74 #inches
eyes = 'Brown'
teeth = 'White'
hair = 'Black'
cenimeters = height * 2.54
kilograms = weight * 0.45

print "Let's talk about %s." % name
print "She's %d inches tall." % height
print "Actually that's not too shabby."
print "She's got %s eyes and %s hair." % (eyes, hair)
print "Her teeth are normally %s depending on the coffee." % teeth
print "Janice is %s cenimeters tall." % cenimeters
print "Janice weights %s kilograms." % kilograms

print "If I add %d and %d I get %d." % (age, height, age + height)
